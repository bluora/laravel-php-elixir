{
    "name": "bluora/laravel-php-elixir",
    "description": "Provides a PHP based equivalent to Node.js based Elixir",
    "keywords": ["laravel", "elixir"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rocco Howard",
            "email": "rocco@bluora.com.au"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-inotify": "*",
        "symfony/yaml": "~3.0",
        "leafo/scssphp": "~0.6.0",
        "matthiasmullie/minify": "~1.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "illuminate/database": "4.*|5.*",
        "symfony/process": "~2.3",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PhpElixir\\": "src/"
        }
    }
}
